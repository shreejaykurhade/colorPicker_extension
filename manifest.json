{
  "manifest_version": 3,
  "name": "Color Picker",
  "description": "Pick any color from a webpage and get its hex code",
  "version": "1.0",
  "permissions": ["storage", "scripting", "activeTab"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "icons": {
    "128": "icon.png"
  }
}
